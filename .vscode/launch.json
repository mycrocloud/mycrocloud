{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "WebMvc",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build_web",
            "program": "${workspaceFolder}/src/Web/MycroCloud.WebMvc/bin/Debug/net8.0/MycroCloud.WebMvc.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/MycroCloud.WebMvc",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS":"https://localhost:5002"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/src/MycroCloud.WebMvc/Views"
            },
            "requireExactSource": false
        }
    ]
}