@using MockServer.Web.Controllers
@model MockServer.Web.Models.Function.FunctionIndexViewModel
@if (Model.Functions.Count() > 0)
{
    <div>
        <a href="@Url.Action(nameof(FunctionsController.Create))">New</a>
    </div>
    <ul>
        @foreach (var function in Model.Functions)
        {
            <li>
                @function.Name
            </li>
        }
    </ul>
}
else 
{
    <div>There is no function</div>
}
