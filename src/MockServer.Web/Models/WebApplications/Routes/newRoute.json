{
  "id": 1,
  "name": "name",
  "description": "description",
  "match": {
    "methods": ["GET", "POST"],
    "path": "path"
  },
  "order": 1,
  "authType": "anonymous|authorized",
  "policyIds": [1, 2, 3, 4, 5],
  "claims": {
    "roles": "administrator",
    "scopes": ["read", "write", "delete"]
  },
  "validate": {
    "queryParameters": {
      "param1": {
        "required": {
          "message": "param1 is required"
        },
        "minlength": {
          "length": 3,
          "message": "please enter at least 3 characters"
        }
      },
      "param2": {
        "maxlength": {
          "length": 4,
          "message": "please enter no more than 4 characters"
        },
        "rangelength": {
          "range": [2, 6],
          "message": "please enter a value between 2 and 6 characters long"
        }
      }
    },
    "headers": {
      "header1": {
        "min": {
          "value": 13,
          "message": "please enter at least 13 characters"
        }
      },
      "header2": {
        "range": {
          "range": [13, 23],
          "message": "please enter a value between 13 and 23 characters long"
        }
      }
    },
    "bodyMaxSize": 1000000,
    "body": {
      ".": {
        "equalTo": {
          "value": "bar",
          "message": "please enter bar"
        }
      },
      ".field1": {
        "digits": {
          "message": "please enter only digits"
        }
      }
    }
  },
  "response": {
    "provider": "mockResponse|transforms|function",
    "mockResponse": {
      "status": 200,
      "delay": {
        "delayed": true,
        "type": "fixed",
        "time": 1000
      },
      "body": {
        "template": "none|jsonstringify|handlebars",
        "textFormat": "json|plain|html|xml",
        "text": "foo"
      },
      "headers": [
        {
          "name": "Content-Type",
          "value": "application/json",
          "type": "static"
        },
        {
          "name": "Content-Length",
          "value": "3",
          "type": "static"
        },
        {
          "name": "Content-Encoding",
          "value": "gzip",
          "type": "dynamic"
        }
      ]
    },
    "transforms": {
      "scheme": "http",
      "host": "localhost",
      "port": 8080,
      "method": {
        "original": true,
        "method": "GET"
      },
      "path": {
        "original": true,
        "addPrefix": "/addPrefix",
        "removePredix": "/removePrefix",
        "path": "/newpath"
      },
      "headers": [
        { 
          "header": "header1", 
          "set": "value1" 
        },
        { 
          "remove": "header2"
        }
      ]
    },
    "function": {
      "id": 1
    }
  }
}
