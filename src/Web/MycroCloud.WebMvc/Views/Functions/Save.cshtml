@model MicroCloud.Web.Models.Function.FunctionSaveModel
<form method="post">
    <div>
        <label asp-for="Name" class="form-label"></label>
        <input type="text" asp-for="Name" class="form-control" placeholder="Function name" autocomplete="off" aria-autocomplete="none">
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="mt-2">
        <label class="form-label">Runtime</label>
        <select asp-for="RuntimeId" asp-items="Model.RuntimeSelectListItem" class="form-select"></select>
        <span asp-validation-for="RuntimeId" class="text-danger"></span>
    </div>
    <div class="mt-2">
        <button type="submit" class="btn btn-primary">Save</button>
    </div>
</form>