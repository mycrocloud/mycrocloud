{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information",
      "WebApp.Gateway.Middlewares.RoutingMiddleware": "Debug"
    },
    "Seq": {
      "ServerUrl": "http://localhost:5341",
      "ApiKey": ""
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Username=postgres;Password=postgres;Database=mycrocloud",
    "Redis": "redis:6379"
  },
  "DockerFunctionExecution": {
    "Uri": "unix:///var/run/docker.sock",
    "Image": "mycrocloud-function-invoker",
    "HostFilePath": "/tmp/function-data"
  },
  "AppNameSource": "Header:X-App-Name",
  "Host": "apigw.mycrocloud.local",
  "Proxy": {
    "Subnet": "172.18.0.0/16"
  },
  "Storage": {
    "RootPath": "data"
  }
}